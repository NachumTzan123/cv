body {
  margin: 0;
  text-align: center;
  font-family: 'Courier Prime', monospace;
}

h1 {
  margin-top: 100px;
  font-family: 'Courgette', cursive;
  font-size: 5.625rem;
  margin-bottom: 20px;
}

h2 {
  font-family: 'Inconsolata', monospace;
  color: #5eaaa8;
}

h3 {
  font-family: 'Inconsolata', monospace;
  color: #056676;
}

.specialp {
  font-family: "Courier New", Courier, "Lucida Sans Typewriter", "Lucida Typewriter", monospace;
  font-size: 1.5rem;
  color: #056676;
  font-weight: lighter;
}

.topdiv {
  background-color: #a3d2ca;
  position: relative;
  padding-top: 100px;
  margin-bottom: 0;
}

.topcloud {
  position: absolute;
  right: 27%
}

.botcloud {
  position: absolute;
  left: 30%;
}

.middiv {
  background-color: #e8ded2;
  position: relative;
}

.botdiv {
  background-color: #e8ded2;
}

.roundimg {
  width: 20%;
  border-radius: 20%;
}

.credit {
  font-size: 7px;
  position: relative;
  bottom: 10px;
}

.link {
  text-decoration: none;
  font-weight: bold;
  color: black;
}

.link:hover {
  color: #5eaaa8;
}

.skill-row{
  width: 50%;
  margin: 0 auto 0 auto;
  line-height: 2;
  font-size: 100%;
}

#profilepic{
  width: 10%;
}
